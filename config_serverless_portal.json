      {
        "inboundTag": [
          "interconn"
        ],
        "domain": ["full:reverse8.proxy"],
        "outboundTag": "portal8"
      },
      {
        "inboundTag": [
          "interconn", "socks-in"
        ],
        "balancerTag": "round"
      } 
    ],
    "balancers" : [
            {
                "selector": [
                    "portal1", "portal2", "portal3", "portal4", "portal5", "portal6", "portal7", "portal8"
                ],
                "strategy": {
                    "type":"roundRobin"
                },
                "tag": "round"
            }
        ]
  }

{
  "log": {
    "loglevel": "warning"
  },
  "reverse": {
    "portals": [
      {
        "tag": "portal1",
        "domain": "reverse1.proxy"
      },
      {
        "tag": "portal2",
        "domain": "reverse2.proxy"
      },
      {
        "tag": "portal3",
        "domain": "reverse3.proxy"
      },
      {
        "tag": "portal4",
        "domain": "reverse4.proxy"
      },
      {
        "tag": "portal5",
        "domain": "reverse5.proxy"
      },
      {

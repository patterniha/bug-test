  "inbounds": [
    {
            "tag": "socks-in",
            "port": "10808",
            "protocol": "socks",
            "settings": {
                "udp": true
            }
    },
    {
      "tag": "interconn",
      "port": ?,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "?"
          }
        ]
      },
      "streamSettings": {
        "network": "tcp"
      }
    }
  ],
  "routing": {
    "rules": [
      {
        "inboundTag": [
          "interconn"
        ],
        "domain": ["full:reverse1.proxy"],
        "outboundTag": "portal1"
      },
      {
        "inboundTag": [
          "interconn"
        ],
        "domain": ["full:reverse2.proxy"],
        "outboundTag": "portal2"
      },
      {
        "inboundTag": [
          "interconn"
        ],
        "domain": ["full:reverse3.proxy"],
        "outboundTag": "portal3"
      },
      {
        "inboundTag": [
          "interconn"
        ],
        "domain": ["full:reverse4.proxy"],
        "outboundTag": "portal4"
      },
      {
        "inboundTag": [
          "interconn"
        ],
        "domain": ["full:reverse5.proxy"],
        "outboundTag": "portal5"
      },
      {
        "inboundTag": [
          "interconn"
        ],
        "domain": ["full:reverse6.proxy"],

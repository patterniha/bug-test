{
  "log": {
    "loglevel": "debug", "dnsLog": true
  },

  "dns":{
    "servers": ["1.1.1.1"],
    "tag": "dns-query"
  },
  
  "inbounds": [
    {
      "tag": "socks-in",
      "port": 10808,
      "protocol": "socks",
      "sniffing": {
        "enabled": true,
        "destOverride": ["quic"]
      },
      "settings": {"udp": true}
    }
  ],

  "outbounds": [
    {
      "tag": "direct",
      "protocol": "freedom"
    },    
    {
      "tag": "dns-out",
      "protocol": "dns"
    }    
  ],

  "routing": {
    "domainStrategy": "AsIs",
    "rules": [                  
      {"outboundTag": "dns-out",
       "inboundTag": ["socks-in"], "port": 53
      },
      {"outboundTag": "direct",
       "port": "0-65535"
      }
    ]
  }
}






